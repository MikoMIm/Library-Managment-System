<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="rootAnchorPane" prefHeight="381.0" prefWidth="717.0" style="-fx-background-color: #242425;"
            xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.mordvinovdsw.library.supportControllers.ImportExportController">
   <HBox alignment="CENTER" spacing="20" AnchorPane.topAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.bottomAnchor="0"
         AnchorPane.leftAnchor="0">
      <padding>
         <Insets top="60" right="20" bottom="60" left="20"/>
      </padding>
      <Button fx:id="ImportButton" onAction="#importDataBase" styleClass="menu-button"
              stylesheets="@../../../../Stylesheet/Style_fastMenu.css" HBox.hgrow="ALWAYS">
         <graphic>
            <VBox alignment="CENTER" spacing="10">
               <ImageView fitHeight="142.0" fitWidth="155.0" preserveRatio="true" pickOnBounds="true">
                  <Image url="@../../../../IMG/import.png"/>
               </ImageView>
               <Label styleClass="menu-label" text="Import"/>
            </VBox>
         </graphic>
      </Button>
      <Button fx:id="ExportButton" onAction="#exportDataBase" styleClass="menu-button"
              stylesheets="@../../../../Stylesheet/Style_fastMenu.css" HBox.hgrow="ALWAYS">
         <graphic>
            <VBox alignment="CENTER" spacing="10">
               <ImageView fitHeight="142.0" fitWidth="155.0" preserveRatio="true" pickOnBounds="true">
                  <Image url="@../../../../IMG/export.png"/>
               </ImageView>
               <Label styleClass="menu-label" text="Export"/>
            </VBox>
         </graphic>
      </Button>
      <Button fx:id="CreateDataButton" onAction="#createDataBase" styleClass="menu-button"
              stylesheets="@../../../../Stylesheet/Style_fastMenu.css" HBox.hgrow="ALWAYS">
         <graphic>
            <VBox alignment="CENTER" spacing="10">
               <ImageView fitHeight="142.0" fitWidth="167.0" preserveRatio="true" pickOnBounds="true">
                  <Image url="@../../../../IMG/createDB.png"/>
               </ImageView>
               <Label styleClass="menu-label" text="Create New DB"/>
            </VBox>
         </graphic>
      </Button>
   </HBox>
</AnchorPane>